<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <url value="https://www.medizininformatik-initiative.de/fhir/core/StructureDefinition/ObservationLab" />
  <name value="ObservationLab" />
  <title value="MI-Initiative - Laborprofile - Observation" />
  <status value="draft" />
  <experimental value="true" />
